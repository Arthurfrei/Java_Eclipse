public class Questão08_TE {

	public static void main(String[] args) {

		int a = 150;

		if (a > 100) {
			if (a < 200) {
				System.out.println(" é verdade!!!");
			} else {
				System.out.println(" não é verdadeiro!!!");
			}
		} else {
			System.out.println(" não é verdadeiro!!!");
		}

	}

}


